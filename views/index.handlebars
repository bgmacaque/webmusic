<body>	
	<div class="row">
		<div class="col-md-5">
			<h1>Tabs</h1>
			{{#each tabs}}
				<li class="list-group-item">
					<a href="/tab/profil/{{id}}">{{name}}</a>
					<a href="/tab/download/{{id}}">Download</a>
					<span class="badge">{{note}}</span>
				</li>			
			{{/each}}
		</div>
		<div class="col-md-5">
		<h3>Top 10 Tabs</h3>
		<ul class="list-group">
			{{#each topTabs}}
				<li class="list-group-item">
					<a href="/tab/profil/{{id}}">{{name}}</a>
					<a href="/tab/download/{{id}}">Download</a>
					<span class="badge">{{note}}</span>
				</li>
			{{/each}}
			</ul>
		</div>
	</div>
</body>