<body>
	<h1>{{title}}</h1>
	<p>You can search a tab on the website</p>
	<form method="GET">
		<input type="search" name="q">
		<input type="submit">
	</form>
	<div class="results">
		{{#unless result}}
			No results
		{{else}}
			{{#if users}}
				<h1>Users</h1>
				<ul class="list-group">
					{{#each users }}
						<li class="list-group-item">
							<a href="/user/profil/{{id}}">{{nickname}}</a>
						</li>
					{{/each}}						
				</ul>
		
			{{/if}}
			{{#if tabs}}
				<h1>Tabs</h1>
				<ul class="list-group">
					{{#each tabs }}
						<li class="list-group-item">
							<a href="/tab/profil/{{id}}">{{name}}</a>
						</li>
					{{/each}}						
				</ul>

			{{/if}}

		{{/unless}}
	</div>
</body>